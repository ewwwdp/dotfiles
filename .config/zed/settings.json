{
  "autosave": "on_focus_change",
  "base_keymap": "VSCode",
  "ui_font_size": 15,
  "buffer_font_size": 14,
  "buffer_font_family": "JetBrains Mono",
  "theme": "VScode Dark Plus",
  "terminal": {
    "font_family": "JetBrainsMono Nerd Font"
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "auto_signature_help": true,
  "inlay_hints": {
    "enabled": true
  },
  "restore_on_startup": "last_workspace",
  "tabs": {
    "file_icons": true
  }
}
